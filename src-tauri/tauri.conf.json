{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Aspenini-Fun",
  "version": "1.0.0",
  "identifier": "com.aspenini.aspenini-fun",
  "build": {
    "frontendDist": "../site",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Aspenini-Fun",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
